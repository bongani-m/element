    "fast-json-patch": "^2.0.7",
    "hdkey": "^1.1.1",
    "ipfs-api": "^26.1.2",
    "ipfs-http-client": "^30.1.1",
    "ipfs-unixfs": "^0.1.16",
    "ipld-dag-pb": "^0.15.3",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.11",
    "merkle-tools": "^1.4.0",
    "multihashes": "^0.4.14",
    "node-localstorage": "^1.3.1",
